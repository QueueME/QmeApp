        <activity android:name="com.app.queueme.Infoscreens.WelcomeActivityStudent"
            android:screenOrientation="portrait"/>

        <activity android:name="com.app.queueme.ResetPasswordActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.app.queueme.MenyActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.app.queueme.LoginActivity"
            android:screenOrientation="portrait"/>

      <activity android:name="com.app.queueme.Studassqueue.StudassQueue"></activity>
      
