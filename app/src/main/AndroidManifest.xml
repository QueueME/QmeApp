<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.queueme">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.app.queueme.Beginning"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.app.queueme.AddPerson"
            />
        <activity android:name="com.app.queueme.AddSubject"
            />
        <activity android:name="com.app.queueme.ChoosePerson"
            android:screenOrientation="portrait" />

        <activity android:name=".MySessionSwipeFunction.ScreenSlidePagerActivity"
